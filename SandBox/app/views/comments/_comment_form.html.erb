<%= form_for([@welcome_page,@welcome_page.comments.build]) do |f| %>
  <div class="field">
      <%=f.label :commenter %><br />
      <%=f.text_field :commenter %>
    </div>
  <div class="field">
      <%=f.label :msg %><br />
      <%=f.text_field :msg %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
